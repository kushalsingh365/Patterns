#include <bits/stdc++.h>
using namespace std;
int main(){
	int n;
	cin>>n;
	int i=1;
	//char start='A';
	while(i<=n){
		int j=1;
		char ch='A'+i+j-2;
		while(j<=n){
			cout<<ch<<" ";
			ch=ch+1;
			j++;
		}
		cout<<endl;
		i++;
	}

}
/*INPUT = 3
OUTPUT
A B C 
B C D 
C D E


Coded by Kushal Singh

i =row;
j=column

*/
