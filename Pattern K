#include <bits/stdc++.h>
using namespace std;
int main(){
	int n;
	cin>>n;
	int i=1;
	//int count=1;
	while(i<=n){
		int j=1;
		//int value=i;
		while(j<=n){
			char ch = 'A'+i-1;
			cout<<ch<<" ";
			//value--;
			j++;
		}
		cout<<endl;
		i++;
	}

}

/*INPUT = 3
OUTPUT
A A A 
B B B 
C C C


Coded by Kushal Singh

i =row;
j=column

*/
