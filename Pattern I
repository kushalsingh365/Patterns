#include <bits/stdc++.h>
using namespace std;
int main(){
	int n;
	cin>>n;
	int i=1;
	//int count=1;
	while(i<=n){
		int j=1;
		int value=i;
		while(j<=i){
			cout<<value<<" ";
			value++;
			j++;
		}
		cout<<endl;
		i++;
	}

}

/*INPUT = 3
OUTPUT
1 
2 3 
3 4 5 


Coded by Kushal Singh

i =row;
j=column

*/
